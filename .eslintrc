{
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "quotes": ["error", "single"]
  }
}
// yarn add -D eslint prettier eslint-config-prettier eslint-plugin-prettier eslint-plugin-import
